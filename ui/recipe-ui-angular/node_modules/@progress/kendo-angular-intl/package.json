{
  "name": "@progress/kendo-angular-intl",
  "version": "17.1.1",
  "description": "Kendo UI Internationalization for Angular components",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Progress",
  "homepage": "https://www.telerik.com/kendo-angular-ui/components/",
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "keywords": [
    "Angular",
    "Internationalization",
    "Kendo UI",
    "i18n"
  ],
  "@progress": {
    "friendlyName": "Internationalization"
  },
  "assets": [
    "locales"
  ],
  "sideEffects": [
    "./locales/**/*"
  ],
  "peerDependencies": {
    "@angular/animations": "16 - 19",
    "@angular/common": "16 - 19",
    "@angular/core": "16 - 19",
    "@angular/platform-browser": "16 - 19",
    "@progress/kendo-angular-common": "17.1.1",
    "@progress/kendo-licensing": "^1.0.2",
    "rxjs": "^6.5.3 || ^7.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "@progress/kendo-angular-schematics": "17.1.1",
    "@progress/kendo-intl": "^3.1.0"
  },
  "schematics": "./schematics/collection.json",
  "exports": {
    "./locales/*": "./locales/*.js",
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/progress-kendo-angular-intl.mjs",
      "esm": "./esm2022/progress-kendo-angular-intl.mjs",
      "default": "./fesm2022/progress-kendo-angular-intl.mjs"
    }
  },
  "module": "fesm2022/progress-kendo-angular-intl.mjs",
  "typings": "index.d.ts"
}